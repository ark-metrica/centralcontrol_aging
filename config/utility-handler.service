#AssertPathExists=/opt/software/source/central-control/central_control/utility_handler.py

[Unit]
After=network-online.target
Wants=network-online.target
Description=The Utility Handler service

[Service]
Environment=PYTHONPATH=/opt/software/python
#ExecStart=python /opt/software/source/central-control/central_control/utility_handler.py
ExecStart=python -m central_control.utility_handler 127.0.0.1
Restart=always
PrivateTmp=true
NoNewPrivileges=true

[Install]
WantedBy=default.target
