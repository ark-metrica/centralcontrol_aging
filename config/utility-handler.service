AssertPathExists=/opt/software/source/central-control/central_control/utility_handler.py

[Unit]
After=network-online.target
Wants=network-online.target
Description=Starts the Utility Handler service

[Service]
#WorkingDirectory=/tmp
Environment=PYTHONPATH=/opt/software/python
ExecStart=python /opt/software/source/central-control/central_control/utility_handler.py
Restart=always
PrivateTmp=true
NoNewPrivileges=true

[Install]
WantedBy=default.target